  <legend>Login</legend>

    <% form_for (@session) do |f| %>
    <label for="session_email">Email</label><br />
    <%= f.text_field :email %><br />
   
    <label for="session_password">Password</label><br />
    <%= f.password_field :password %><br />

    <%= submit_tag "login" %>

  <% end %>
  