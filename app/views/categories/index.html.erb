<div class="container">
<table>
  <tr>
    <th><%= t :name %></th>
  </tr>

<% @categories.each do |category| %>
  <tr>
    <td><%= category.name %></td>
    <td><%= link_to (t :show), category %></td>
     <td><%= link_to (t :edit), edit_category_path(category) %></td>
    <td><%= link_to (t :destroy), category, method: :delete, data: { confirm: (t :you_sure) + "?"  } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to (t :new_category), new_category_path %>
